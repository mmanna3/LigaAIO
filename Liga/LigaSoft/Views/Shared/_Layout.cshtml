<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>EDEFI</title>
	
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")

	@Styles.Render("~/gijgocss")
	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@Scripts.Render("~/gijgojs")	
	@Scripts.Render("~/bundles/fontAwesome")	
	
</head>
<body>
	
@Html.Partial("_TopBar")

@Html.Partial("_SideBar")

<div class="main">	
	<h2 class="titulo">@RenderSection("Titulo", required: true)</h2>
	<h4>@RenderSection("Subtitulo", required: false)</h4>
	<div class="row cabecera-grilla no-imprimir">@RenderSection("CabeceraGrilla", required: false)</div>
	@RenderBody()
</div>	
    @RenderSection("scripts", required: false)
</body>
</html>
